{
  "name": "Send To Bishou APP",
  "version": "2.0",
  "description": "将链接收藏至必收!",
  "manifest_version": 2,
  "background": {
    "scripts": ["js/background-main.js"]
  },
  "browser_action": {
    "default_icon": {
      "128": "images/icon.png"
    },
    "default_popup": "",
    "default_title": "收藏至必收"
  },
  "icons": {
    "128": "images/icon.png",
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "64": "images/icon.png",
    "96": "images/icon.png"
  },
  "options_page": "",
  "permissions": [
    "nativeMessaging",
    "clipboardWrite",
    "\u003Call_urls>",
    "activeTab",
    "contextMenus",
    "storage",
    "http://*/*",
    "https://*/*",
    "tabs",
     "notifications"
  ],
  "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx"
}
